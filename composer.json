{
    "name": "victorycto/php-youtube-api",
    "type": "library",
    "description": "PHP wrapper for the Youtube Data API v3 - cloned from madcoda/php-youtube-api to fix an issue",
    "keywords": ["youtube", "api", "video"],
    "license": "MIT",
    "authors": [
        {
            "name": "VictoryCTO",
            "email": "jwc@victorycto.com"
        }
    ],
    "support":{
            "email":"jwc@madcoda.com",
            "issues":"https://github.com/VictoryCTO/php-youtube-api/issues"
    },
    "require": {
        "php" : ">=5.3",
        "ext-curl": "*"
    },
    "require-dev":{
        "phpunit/phpunit": "^4.5 || ^5.0.5"
    },
    "autoload": {
        "psr-4": {
            "Madcoda\\Youtube\\": "src"
        },
        "classmap": ["src/compat.php"]
    }
}
